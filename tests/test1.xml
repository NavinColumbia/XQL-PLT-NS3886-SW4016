
<query>
   <select>
      <column>"class"</column>
      <column>
         <count_func>"*"</count_func>
      </column>
      <column>
         <alias>
            <lhs>
               <max_func>"stats"</max_func>
            </lhs>
            <rhs>"max_stats"</rhs>
         </alias>
      </column>
   </select>
   <from>
      <table>"characters"</table>
      <table>"games"</table>
   </from>
   <where>
      <eq_op>
         <lhs>
            <ref_table>"games"</ref_table>
            <ref_col>"game"</ref_col>
         </lhs>
         <rhs>
            <string_constant>"'genshin'"</string_constant>
         </rhs>
      </eq_op>
      <and/>
      <bracket>
         <eq_op>
            <lhs>
               <ref_table>"games"</ref_table>
               <ref_col>"origin"</ref_col>
            </lhs>
            <rhs>
               <string_constant>"'USA'"</string_constant>
            </rhs>
         </eq_op>
         <or/>
         <eq_op>
            <lhs>
               <ref_table>"games"</ref_table>
               <ref_col>"origin"</ref_col>
            </lhs>
            <rhs>
               <string_constant>"'Japan'"</string_constant>
            </rhs>
         </eq_op>
      </bracket>
   </where>
   <group_by>
      <column>"class"</column>
   </group_by>
   <having>
      <gt_op>
         <lhs>
            <count_func>
               <column>"*"</column>
            </count_func>
         </lhs>
         <rhs>
            <int_constant> 3 </int_constant>
         </rhs>
      </gt_op>
   </having>
   <order_by>
      <asc>
         <ref_col>"class"</ref_col>
      </asc>
   </order_by>
</query>